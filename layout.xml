<?xml version="1.0" encoding="utf-8"?>
<romset>
   <resources>
      <rom id="romimage" file="romc.bin"/>
   </resources>
   <configuration>
      <!-- Cartridge with a paged ROM of up to 32 MB (as of Mame 0.244) -->
      <pcb type="gromemu">
         <socket id="rom_socket" uses="romimage"/>
      </pcb>
   </configuration>
</romset>
